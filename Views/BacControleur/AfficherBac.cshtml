        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.idBac)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.idBac)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.longitude)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.longitude)
        </dd>
