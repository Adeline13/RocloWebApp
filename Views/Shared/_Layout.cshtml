<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - RocloWebApp</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <!--<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/img/favicon.png" rel="icon">
    <link href="~/img/apple-touch-icon.png" rel="apple-touch-icon">-->
    <!-- Google Fonts -->
    <link rel="stylesheet" href="https://opensource.keycdn.com/fontawesome/4.7.0/font-awesome.min.css" integrity="sha384-dNpIIXE8U05kAbPhy3G1cz+yZmTzA6CY8Vg/u2L9xRnHjJiAK76m2BIEaSEV+/aU" crossorigin="anonymous">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/vendor/aos/aos.css" rel="stylesheet">
    <link href="~/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">

    <link href="~/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">
    <link href="~/css/site.css" rel="stylesheet">
    <link href="~/img/favicon.ico" rel="icon">
    <link href="~/img/apple-touch-icon.png" rel="apple-touch-icon">
    <!-- <link href="~/css/css/pageGraphique.css" />-->
    <!--<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet">-->
    <link href="~/lib/font-awesome/css/all.css" rel="stylesheet" />
</head>
<body>
    <div class="position-relative d-flex h-100">
        <header class="wrapper position-fixed d-flex vh-100" style="background-color: #fff; z-index: 1;">
            <div>
                <nav id="navbar2" class="navbar navbar-expand-sm navbar-light h-100 border-bottom box-shadow flex-column vh-100 text-white" style="width : 300px;">
                    <div class="container flex-column text-left">
                        <a class="navbar-brand w-100 text-center" asp-area="" asp-controller="RocloFrontControleur" asp-action="Accueil">Roclo Web App</a>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">RocloWebApp</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <div class="collapse d-sm-inline-flex flex-sm-row-reverse ">
                            <ul id="marge" class="navbar-nav flex-column nav-pills">

                        <li class="nav-item">
                                    <a class="nav-link  oi oi-list-rich" asp-area="" asp-controller="RocloFrontControleur" asp-action="PageGraphique"><i class="fas fa-tachometer-alt"></i>  Tableau de bord </a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="AdminControleur" asp-action="Index"><i class="fas fa-user-cog"></i>  Admin</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="BacControleur" asp-action="Index"><i class="fas fa-dumpster"></i>  Bac</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="ClientControleur" asp-action="Index"><i class="fas fa-user-cog"></i>  Client</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="CollecteurControlleur" asp-action="Index"><i class="fas fa-user-cog"></i>  Collecteur</a>
                                </li>
                                <li class="nav-item">
                                    <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="CommandeControleur" asp-action="Index"><i class="fas fa-cash-register"></i> Commande</a>
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                                    <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="ProduitControleur" asp-action="Index"><i class="fa fa-cogs" aria-hidden="true"></i> Produit</a>
                        </li>
                    </ul>
                </div>
                    </div>
                </nav>
            </div>

        </header><!-- fin header -->

        <div id="conteneur" class="w-100" style="margin-left: 300px;">
            <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light border-bottom box-shadow mb-3 h-100">
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between end-0">
                    <ul class="navbar-nav flex-grow-1 text-sm-right">
                        <li class="nav-item">
                            <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="RocloFrontControleur" asp-action="Accueil"><i class="fa fa-home fa-fw" aria-hidden="true"></i> Accueil</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link oi oi-list-rich" asp-area="" asp-controller="RocloFrontControleur" asp-action="Accueil"><i class="fa fa-info-circle fa-fw" aria-hidden="true"></i> A propos</a>
                        </li>
                    </ul>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            <main role="main" class="container" id="main">
            @RenderBody()
        </main>
    </div>
    </div>

    <!--<footer class="border-top footer text-muted">
    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2021 - RocloWebApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            &copy; 2021 - RocloWebApp - <a asp-area="" asp-controller="Home" asp-action="Privacy">Aide</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    </footer>-->

    @RenderSection("Scripts", required: false)


    <!-- Vendor JS Files -->
    <script src="~/vendor/aos/aos.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/vendor/php-email-form/validate.js"></script>
    <script src="~/vendor/purecounter/purecounter.js"></script>
    <script src="~/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/popper.min.js"></script>
    <script src="~/js/site.js"></script>
    <!--<script src="~/js/pageGrapfique.js"></script>
    <script src="~/js/main.js"></script>-->
    <!-- Template Main JS File -->
    <script src="~/js/main.js"></script>
</body>
</html>
